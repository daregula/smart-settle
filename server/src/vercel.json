{
    "version": 2,
    "builds": [
        {
            "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
        },
        {
            "src": "./api/index.js",
            "use": "@vercel/node"
        }
        ],
        "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api"
        }
        ]
}