{
    "version": 2,
    "builds": [
        {
            "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
        },
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
        ],
        "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
        ]
}