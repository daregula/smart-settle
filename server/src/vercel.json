{
    "version": 2,
    "builds": [
        {
            "rewrites": [{ "source": "/api/(.*)", "destination": "/server/api/index.js" }]
        },
        {
            "src": "./api/index.js",
            "use": "@vercel/node"
        }
        ],
        "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/api/index.js"
        }
        ]
}