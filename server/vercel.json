{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
    ],
    "includeFiles": ["server/public/sample_data/cost_of_living.json", "server/public/sample_data/points_of_interest.json"],

    "functions": {
      "./src/routes/result.js":{
        "includeFiles": ["./public/sample_data/cost_of_living.json", "./public/sample_data/points_of_interest.json"]
      }
    }
}