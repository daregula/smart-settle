{
    "version": 2,
    
    "builds": [
      {
        "src": "/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["/public/sample_data/cost_of_living.json", "/public/sample_data/points_of_interest.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
    ]
}